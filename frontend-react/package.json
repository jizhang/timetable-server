{
  "name": "timetable",
  "version": "0.1.0",
  "private": true,
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "start:local": "cross-env MOCK=none parcel",
    "build": "parcel build",
    "noprepare": "is-ci || (cd .. && husky install frontend/.husky)",
    "lint": "eslint --ext .js,.ts,.tsx --fix . && prettier --write .",
    "lint:ci": "eslint --ext .js,.ts,.tsx . && prettier --check .",
    "typecheck": "tsc --noEmit",
    "gen": "openapi-generator-cli generate"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fullcalendar/interaction": "^5.11.0",
    "@fullcalendar/react": "^5.11.1",
    "@fullcalendar/timegrid": "^5.11.0",
    "dayjs": "^1.11.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.2",
    "react-query": "^3.39.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "body-parser": "^1.20.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "glob": "^8.0.2",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.4.1",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "routes": "^2.1.0",
    "send-data": "^8.0.0",
    "typescript": "^4.6.4"
  }
}
